{
	"name": "SPOJ Problems Score",
	
	"version": "0.0.0.1",
	
	"manifest_version": 2,
	
	"description": "try",
	
	"author": "Preet Thakkar",
	
	"browser_action": {
		"default_icon": "src/img/favicon_spoj.png",
		"default_title": "SPOJ Problem Score",
		"default_popup": "src/popup.html"
	},
	
	"content_scripts": [
		{
			"matches": ["https://www.spoj.com/problems/*"],
			"js": ["src/content.js"]
		}
	]
}